# Copy to .env and adjust values for your environment.

# Database connection (compatible with manufacturing_db container)
# POSTGRES_URL=postgresql://user:pass@host:5432/dbname
# or specify components:
# POSTGRES_USER=
# POSTGRES_PASSWORD=
# POSTGRES_DB=
# POSTGRES_HOST=localhost
# POSTGRES_PORT=5432

# Auth / JWT
JWT_SECRET_KEY=CHANGE_ME
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_MINUTES=20160  # 14 days

# App
ENVIRONMENT=dev
CORS_ORIGINS=*
